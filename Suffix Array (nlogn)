#include <bits/stdc++.h>
#define fore(i,a,b) for(int i=a;i<b;i++)
#define mp make_pair
#define vi vector<int>
using namespace std;
vi sort_suffixes(string &s){
	int n=s.size();
	vi cnt(max(256,n)),c(n),p(n);
	fore(i,0,n) cnt[s[i]]++;
	fore(i,1,256) cnt[i]+=cnt[i-1];
	for(int i=n-1;i>=0;i--) p[--cnt[s[i]]]=i;
	int classes=1;
	c[p[0]]=0;
	fore(i,1,n){
		if(s[p[i]]!=s[p[i-1]]) classes++;
		c[p[i]]=classes-1;
	}
	vi pn(n),cn(n);
	for(int h=0;(1<<h)<n;h++){
		fore(i,0,n) {
			pn[i]=p[i]-(1<<h);
			if(pn[i]<0) pn[i]+=n;
		}
		fill(cnt.begin(),cnt.begin()+classes,0);
		fore(i,0,n) cnt[c[pn[i]]]++;
		fore(i,1,classes) cnt[i]+=cnt[i-1];
		for(int i=n-1;i>=0;i--) p[--cnt[c[pn[i]]]]=pn[i];
		cn[p[0]]=0;
		fore(i,1,n){
			if(mp(c[p[i]],c[(p[i]+(1<<h))%n])!=mp(c[p[i-1]],c[(p[i-1]+(1<<h))%n])) classes++;
			cn[p[i]]=classes-1;
		}
		c.swap(cn);
	}
	return p;
}
int main() {
	string s;
	cin>>s;
	int n=s.size();
	vi p=sort_suffixes(s);
	fore(i,0,n) cout<<p[i]<<" ";
	
	return 0;
}
